{
  "name": "rename-me",
  "version": "0.0.1",
  "scripts": {
    "get-joomla": "joomla-tools.mjs -i",
    "link": "joomla-tools -l",
    "build": "joomla-tools -b media_source",
    "release": "joomla-tools -r",
    "init": "joomla-tools -i && joomla-tools -l && joomla-tools -b media_source",
    "lint-css": "./node_modules/stylelint/bin/stylelint.js ./media_source/.",
    "lint-js": "./node_modules/eslint/bin/eslint.js ./media_source/**/*.mjs",
    "lint": "npm run lint-css && npm run lint-js",
    "update:browserlist": "npx browserslist@latest --update-db"
  },
  "devDependencies": {
    "@dgrammatiko/joomla-tools": "github:dgrammatiko/joomla-tools#main"
  },
  "joomla-extensions": {
    "components": [],
    "libraries": [],
    "modules": {
      "administrator": [
        { "name": "publicfolder", "version": "0.0.1" }
      ],
      "site": []
    },
    "plugins": {
      "actionlog": [],
      "api-authentication": [],
      "authentication": [],
      "behaviour": [],
      "captcha": [],
      "content": [],
      "editors": [],
      "editors-xtd": [],
      "extension": [],
      "fields": [],
      "filesystem": [],
      "finder": [],
      "installer": [],
      "media-action": [],
      "multifactorauth": [],
      "privacy": [],
      "quickicon": [],
      "sampledata": [],
      "system": [],
      "task": [],
      "user": [],
      "webservices": [],
      "workflow": []
    },
    "templates": {
      "administrator": [],
      "site": []
    }
  },
  "joomlaVersion": "5.0.0-beta1"
}
